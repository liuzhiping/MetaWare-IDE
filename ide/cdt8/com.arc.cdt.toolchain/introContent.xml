  <page style="css/startwork/startwork.css" alt-style="css/startwork/swt.properties" id="startwork" style-id="page">    
    <img id="welcome_img" src="css/graphics/contentpage/welcome.jpg" alt="Welcome to the MetaWare IDE"></img>
    <title style-id="intro-header"> </title>
    <group style-id="upper-background" filteredFrom="swt"/>
    <group style-id="navigation-image" filteredFrom="swt"/>
    <group style-id="curve-image" filteredFrom="swt"/>    
    <group style-id="welcome-image" filteredFrom="swt"/>
    <group style-id="page-content">
      <img style-id="startwork-img" src="css/graphics/contentpage/startwork.gif" alt="Start Working with the MetaWare IDE"></img>

      <group  style-id="startwork-group">
	<link label="Create a New Project" url="http://org.eclipse.ui.intro/runAction?pluginId=com.arc.cdt.toolchain&amp;class=com.arc.intro.NewProjectIntroAction" id="mm-startwork" style-id="startwork-link" >
          <text>Create a new project in the currently loaded workspace.</text>
        </link>        
        <link label="Create a New Workspace" url="http://org.eclipse.ui.intro/runAction?pluginId=com.arc.cdt.toolchain&amp;class=com.arc.intro.NewWorkspaceIntroAction" 
	id="mm-startwork" style-id="startwork-link" >
	          <text>Create a new workspace.  You can then create custom projects and begin work.</text>
        </link>
        <link label="Open an Existing Workspace" url="http://org.eclipse.ui.intro/runAction?pluginId=com.arc.cdt.toolchain&amp;class=com.arc.intro.OpenWorkspaceIntroAction" 
	id="mm-startwork" style-id="startwork-link" >
	          <text>Browse for an existing workspace or sample workspace.</text>
        </link>

      </group>

    </group>

  </page>

 
