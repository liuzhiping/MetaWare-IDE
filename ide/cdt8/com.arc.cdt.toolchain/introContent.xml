        <group style-id="overview-links">            
            <link label="Suggested Tutorial" url="http://org.eclipse.ui.intro/showHelpTopic?id=/com.arc.ide.help/help/mw_w_que.htm" id="tutorials">
                <text>Learn how to create C projects, build applications, and debug them.</text>
            </link>
            <link label="Project and Code Samples" url="http://org.eclipse.ui.intro/showPage?id=toolsets" id="samples">
                <text>Load sample projects that you can build, run, and debug</text>
            </link>
            <link label="Start Working" url="http://org.eclipse.ui.intro/showPage?id=startwork" id="startwork">
                <text>Begin working with workspaces and projects</text>
            </link>
        </group>
    </group>
  </page>
 
