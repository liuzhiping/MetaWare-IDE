<?xml version="1.0" encoding="UTF-8"?>
<options>
    <!-- We map the ARC-specific compiler options to their 
         corresponding SeeCode equivalent. -->

    <option name="arc4.compiler.options.bs" property="ARC_barrel_shifter" />
    <option name="arc4.compiler.options.mult32" property="ARC_mult32" />
    <option name="arc4.compiler.options.swap" property="ARC_swap" />
    <option name="arc4.compiler.options.norm" property="ARC_norm" />
    <option name="arc4.compiler.options.minmax" property="ARC_min_max" />
    <option name="arc4.compiler.options.adds" property="ARC_adds" />
    <option name="arc4.compiler.options.xlib" property="ARC_xlib" />
    <option name="arc4.compiler.options.timers.timer0" property="ARC_timer0" 
    		alternate="IDE_ARC_timer0"/>
    <option name="arc4.compiler.options.timers.timer1" property="ARC_timer1" 
    		alternate="IDE_ARC_timer1" />

    <property name="which_arc" value="ARC4"/>
    <property name="which_arc_name" value="ARC 4"/>

    <option name="arc4.compiler.options.xmac" >
	   <enum name="arc4.compiler.options.xmac.noxmac" property="ARC_noxmac" />
	   <enum name="arc4.compiler.options.xmac.xmac16" property="ARC_xmac_16" /> 
	   <enum name="arc4.compiler.options.xmac.xmac24" property="ARC_xmac_24" />
	   <enum name="arc4.compiler.options.xmac.xmacd16" property="ARC_xmac_d16"  />
	   <enum name="arc4.compiler.options.xmac.mulmac" property="ARC_mul_mac"/> 
    </option>


    <option name="arc4.compiler.options.arc4core" property="ARC_Core_Version">
	<enum name="arc4.compiler.options.arc4core.core5" seecode="-core5" propertyValue="5" />
	<enum name="arc4.compiler.options.arc4core.core6" seecode="-core6" propertyValue="6" />
	<enum name="arc4.compiler.options.arc4core.core7" seecode="-core7" propertyValue="7" />
	<enum name="arc4.compiler.options.arc4core.core8" seecode="-core8" propertyValue="8" />
    </option>
	    

</options>
